#include<iostream>
using namespace std;
int main() {
   int search_no, c, n=6;
   int arr[] = { 12, 35, 69, 74, 165, 54};
   search_no=165;
   for (c = 0; c < n; c++) {
      if (arr[c] == sea) {
         printf("%d is present at location %d.\n", search_no, c+1);
         break;
      }
   }
   if (c == n)
      printf("%d isn't present in the array.\n", search_no);
   return 0;
}
